   <center>
      <FlowPane hgap="10.0" prefHeight="200.0" prefWidth="200.0" vgap="20.0" BorderPane.alignment="CENTER">
         <children>
            <Button alignment="CENTER" mnemonicParsing="false" onAction="#goElectrical" prefHeight="100.0" prefWidth="120.0" text="ELECTRICAL DEP." />
